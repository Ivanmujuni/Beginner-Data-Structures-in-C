#include<stdio.h>
#include<stdlib.h>
#include<stdbool.h>
#include<limits.h>

#define MYSTACK_LENGTH 5
#define EMPTY -1
#define STACK_EMPTY INT_MIN

int mystack[MYSTACK_LENGTH];
int top = EMPTY;

bool push(int value){
	if (top >= MYSTACK_LENGTH-1)return false;
	top++;
	mystack[top] = value;
	return true;
}

int pop(){
	if(top == EMPTY)return STACK_EMPTY;
	int result = mystack[top];
	top--;
	return result;
}

int main(){
	push (78);
	push(69);
	push(83);
	
	int t;
	while((t = pop()) != STACK_EMPTY){
		printf("t: %d\n", t);
	}
	
	return 0;
}
